<odoo>
  <data>
  
  	<record id="partner_netcom" model="ir.ui.view">
	    <field name="name">partner.netcom</field>
	    <field name="model">res.partner</field>
	    <field name="inherit_id" ref="base.view_partner_form"/>
	    <field name="arch" type="xml">
	    <field name="lang" position="after">
    		<field name="parent_account_number" />
		</field>
	    </field>
	</record>
	
	<record id="pipeline_netcom_form" model="ir.ui.view">
	    <field name="name">pipeline.netcom.form</field>
	    <field name="model">crm.lead</field>
	    <field name="inherit_id" ref="crm.crm_case_form_view_oppor"/>
	    <field name="arch" type="xml">
	    <field name="user_id" position="after">
    		<field name="nrc"/>
    		<field name="mrc"/>
		</field>
	    </field>
	</record>
	
	<record id="netcom_product_form_view" model="ir.ui.view">
        <field name="name">netcom.product.form</field>
        <field name="model">product.template</field>
        <field name="priority" eval="7" />
        <field name="inherit_id" ref="product.product_template_form_view"/>
        <field name="arch" type="xml">
            <field name="categ_id" position="after">
                <field name="brand"/>
                <field name="equipment_type"/>
                <field name="desc"/>
            </field>
        </field>
    </record>
    
    <record model="ir.actions.act_window" id="brands_action_window">
      <field name="name">Brand Types</field>
      <field name="res_model">brand.type</field>
      <field name="view_mode">tree,form</field>
    </record>
    
    <record model="ir.actions.act_window" id="equipment_action_window">
      <field name="name">Equipment Type</field>
      <field name="res_model">equipment.type</field>
      <field name="view_mode">tree,form</field>
    </record>
    
    <menuitem name="Brand" id="brand_list" parent="stock.menu_product_in_config_stock"
              action="brands_action_window"/>
    <menuitem name="Equipment Types" id="equipment_list" parent="stock.menu_product_in_config_stock"
              action="equipment_action_window"/>
    
    
    
    
    
    
    
    
    
    
